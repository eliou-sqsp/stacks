            "20191:20191",
            "20192:20192",
            "20193:20193",
            "20194:20194",
            "20195:20195",
            "80:20193",
            "443:20194"
          ]
        },
        "brickredis": {
          "name": "brickredis",
          "containerName": "brick-redis",
          "ports": [
            "20196:6379"
          ]
        }
      }
    },
    "postgres": {
      "disabled": true
    },
    "search": {
      "disabled": true
    },
    "elastic": {
      "name": "Elastic",
      "disabled": true
    },
    "legacy-elastic": {
      "disabled": false
